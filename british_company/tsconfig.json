{
  "compilerOptions": {
    "target": "ES6",
    "module": "es6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./src/js"
  },
  "include": ["src/ts/*.ts"],
  "exclude": [
    "node_modules"
  ],
  "moduleResolution": "Node",

}